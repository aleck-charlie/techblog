<div>Hello Tech Blog</div>

{{#each posts as |post|}}
  <article class='card'>
    <div class='card-header flex-row justify-space-between'>
      <h2>
        {{post.title}}
      </h2>
      <div>
        Posted by
        {{post.user.username}}
      </div>
    </div>
    <div class='card-body'>{{post.body}}</div>
  </article>

{{/each}}